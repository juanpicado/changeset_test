{
  "name": "changeset_test",
  "private": true,
  "scripts": {
    "changeset": "changeset",
    "changeset:check": "changeset status --since-master",
    "ci:version": "run-s ci:version:changeset ci:version:repo ci:version:install",
    "ci:version:changeset": "changeset version",
    "ci:version:repo": "run-s fix:repo update:workspace",
    "ci:version:install": "pnpm install --frozen-lockfile=false",
    "fix:repo": "manypkg fix",
    "update:workspace": "pnpm up -r --workspace \"@scope/*\"",
    "build": "preconstruct build",
    "build:dev": "preconstruct dev",
    "postinstall": "preconstruct dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanpicado/changeset_test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juanpicado/changeset_test/issues"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "homepage": "https://github.com/juanpicado/changeset_test#readme",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-typescript": "^7.10.4",
    "@changesets/changelog-github": "^0.1.1",
    "@changesets/cli": "^2.9.2",
    "@manypkg/cli": "^0.14.0",
    "@preconstruct/cli": "1.1.17",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
